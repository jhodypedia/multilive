{
  "name": "yt-multiloop-wib",
  "version": "1.0.0",
  "description": "YouTube multi-channel auto-loop streamer (WIB) with AJAX + Socket.IO + schedule",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:sync": "node -e \"require('./src/config/sequelize').sequelize.sync({alter:true}).then(()=>console.log('synced')).catch(console.error)\""
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "dayjs-plugin-timezone": "^0.1.2",
    "dayjs-plugin-utc": "^0.1.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "helmet": "^7.1.0",
    "mysql2": "^3.11.0",
    "node-cron": "^3.0.3",
    "sequelize": "^6.37.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
